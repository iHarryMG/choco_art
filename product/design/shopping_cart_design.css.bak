body{
margin: 0px;
background-color: #fffef5;
}


/*------------------ HEADERMENU-START ------------------*/
div.header{
z-index: 3;
width: 1280px;
height: 190px;
position: absolute;
left: 50%;
top: 0px;
margin-left:-640px;
}

/*------------------ HEADERMENU-END ------------------*/
/*------------------ LOGINMENU-START-------------------*/
div#loginmenu{
z-index: 4;
position: absolute;
left: 835px;
top: 0px;
margin-top: 0px;
}

div#loginmenu ul{
list-style-type: none;
font-family: "Arial Unicode MS";
font-size: 10px;
}

div#loginmenu ul li{
display: inline;
padding-left: 2px;
color: white;
}

div#loginmenu ul li a{
text-decoration: none;
color: white;
}
/*------------------ LOGINMENU-END -------------------*/
/*------------------ LOGOSCRIPT-START ------------------*/
div#logoscript img{
position: absolute;
top: 10px;
left: 1000px;
}
/*------------------ LOGOSCRIPT-END --------------------*/
/*------------------ CONTENT-START ------------------*/
div.title_script{
position: absolute;
left: 50%;
z-index: 2;
margin-top: 130px;
margin-left: -540px;
}

div.content{
position: absolute;
width: 1280px;
left: 50%;
margin-left:-640px;
min-height:500px;
background-image: url("../../images/background_images/background.jpg");
background-repeat: repeat-y;
background-position: center center;
}

div.content img#title{
position: absolute;
z-index: 1;
width: 450px;
margin-top: 120px;
margin-left: 500px;
}

div.content img#title_image{
position: absolute;
z-index: 0;
margin-top: 90px;
width: 1280px;
left: 50%;
margin-left:-640px;
}

div.content table{
border-collapse: collapse;
margin-top: 300px;
margin-left: 100px;
margin-bottom: 10px;
color: #1a2d0b;
width: 800px;
font-size: 14px;
}

div.content table td, div.content table img{
border: 1px solid #aacf8c;
}

div.content table td input#btn_delete{
border: 1px solid #aacf8c;
font-size: 10px;
}

div.content table td#table_titles{
background-color: #e0ff9e;
color: #1a2d0b;
font-size: 12px;
}

table td#page_number a{
text-decoration: none;
color: #1b3127;
}

div#btns{
margin-left: 600px;
margin-bottom: 30px;
}

table td#billpart{
font-size: 13px;
background-color: #d8e4be;
}

strong{
font-size: 16px;
}

strong#totalpayment{
font-size: 20px;
color: #ff432a;
}

/*------------------ CONTENT-END --------------------*/
/*------------------ SIDEMENU-START ------------------*/
div.submenu_top{
position: absolute;
z-index: -1;
margin-top: -43px;
margin-left: -37px;
}

div#sidemenu{
position: absolute;
z-index: 0;
left: 50%;
margin-left:365px;
top: 130px;
height: 600px;
}

div.sidelogo{
margin-left: 15px;
margin-bottom: 15px;
}
/*------------------ SIDEMENU-END --------------------*/
/*----------------- FOOTERMENU-START -------------------*/
div#footermenu{
background-image: url("../../images/background_images/footer_background.png");
height: 100px;
width: 1280px;
z-index: 3;
padding-top: 20px;
position: relative;
}

div#footermenu ul{
font-size: 12px;
}

div#footermenu ul li{
display: inline;
padding-right: 20px;
color: white;
}

div#footermenu ul li a{
text-decoration: none;
color: white;
font-family: "Arial Unicode MS";
}
/*----------------- DISCLAIMER-START -------------------*/
div#disclaimer{
margin-bottom: 10px;
}
/*----------------- DISCLAIMER-END ---------------------*/

/*----------------- FOOTERMENU-END ---------------------*/